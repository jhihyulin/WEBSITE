{
  "hosting": {
    "public": "build",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "**",
        "destination": "/index",
        "type": 301
      }
    ],
    "cleanUrls": true,
    "trailingSlash": false
  }
}
